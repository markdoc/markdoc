{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "debug typescript file",
      "skipFiles": ["<node_internals>/**"],
      "runtimeArgs": ["-r", "ts-eager/register", "./node_modules/.bin/jasmine"],
      "console": "integratedTerminal",
      "program": "src/tokenizer/plugins/annotations.test.ts",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run marktest",
      "skipFiles": ["<node_internals>/**"],
      "runtimeArgs": ["-r", "ts-eager/register"],
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/spec/marktest/index.ts",
      "cwd": "${workspaceFolder}/spec/marktest",
      "args": ["tests.yaml"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run marktest at Line",
      "skipFiles": ["<node_internals>/**"],
      "runtimeArgs": ["-r", "ts-eager/register"],
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/spec/marktest/index.ts",
      "cwd": "${workspaceFolder}/spec/marktest",
      "args": ["${file}:${lineNumber}"]
    }
  ]
}
